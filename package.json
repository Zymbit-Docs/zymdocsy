{
  "name": "zymbit-docs",
  "version": "0.1.0",
  "license": "CC-BY-SA-4.0",
  "private": true,
  "hugo-bin": {
    "buildTags": "extended"
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "create": "hugo new",
    "prestart": "npm run clean",
    "start": "hugo server --disableFastRender --cleanDestinationDir --enableGitInfo --ignoreCache --noHTTPCache",
    "prebuild": "npm run clean",
    "build": "hugo --gc --minify",
    "build:preview": "npm run build -D -F",
    "clean": "rimraf public resources functions docs",
    "dev:serve": "run-p --race dev:build:watch dev:webserver",
    "dev:serve:poll": "npm-run-all --parallel --race dev:build:poll dev:webserver",
    "dev:build": "hugo --cleanDestinationDir",
    "dev:build:watch": "run-s \"dev:build -- --watch\"",
    "dev:build:poll": "run-s \"dev:build -- --watch --poll 10s\"",
    "dev:webserver": "browser-sync start --no-open --server --watch --port 1313 --serveStatic public/",
    "lint": "npm run -s lint:markdown",
    "lint:markdown": "markdownlint *.md content/**/*.md",
    "release": "standard-version",
    "test": "npm run -s lint",
    "env": "env"
  },
  "devDependencies": {
    "@babel/cli": "^7.12",
    "@babel/core": "^7.12",
    "@babel/preset-env": "^7.12",
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "autoprefixer": "^10.2",
    "bootstrap": "^4.5",
    "browser-sync": "^2.27.5",
    "clipboard": "^2.0",
    "eslint": "^8.0",
    "flexsearch": "^0.6",
    "highlight.js": "^10.7",
    "hugo-bin": "^0.76.1",
    "hugo-extended": "^0.88.1-patch1",
    "instant.page": "^5.1",
    "lazysizes": "^5.3",
    "markdownlint-cli": "^0.29",
    "mermaid": "^8.9.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-cli": "^9.0",
    "purgecss-whitelister": "^2.4",
    "rimraf": "^3.0",
    "shx": "^0.3.3",
    "standard-version": "^9.1",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0"
  },
  "dependencies": {
    "cssnano": "^5.0.8",
    "lunr": "^2.3.9",
    "postcss-import": "^14.0.2"
  }
}
